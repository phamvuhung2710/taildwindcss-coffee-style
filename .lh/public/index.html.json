      <a href=\"#\" class=\"flex items-center\">\r\n                  <svg\r\n                    xmlns=\"http://www.w3.org/2000/svg\"\r\n                    class=\"vhd-icon\"\r\n                    fill=\"none\"\r\n                    viewBox=\"0 0 24 24\"\r\n                    stroke=\"currentColor\"\r\n                    stroke-width=\"2\"\r\n                  >\r\n                    <path\r\n                      stroke-linecap=\"round\"\r\n                      stroke-linejoin=\"round\"\r\n                      d=\"M16 11V7a4 4 0 00-8 0v4M5 9h14l1 12H4L5 9z\"\r\n                    />\r\n                  </svg>\r\n                  <span class=\"mx-2\">Cart</span>\r\n                  <span class=\"text-white bg-orange-400 vhd-badge-circle\"\r\n                    >10</span\r\n                  >\r\n                </a>\r\n              </li>\r\n            </ul>\r\n\r\n            <div\r\n              class=\"flex items-center px-2 cursor-pointer basis-1/6 lg:hidden sm:px-8\"\r\n            >\r\n              <svg\r\n                id=\"vhd-toggle-top-menu-icon\"\r\n                xmlns=\"http://www.w3.org/2000/svg\"\r\n                class=\"vhd-icon\"\r\n                fill=\"none\"\r\n                viewBox=\"0 0 24 24\"\r\n                stroke=\"currentColor\"\r\n                stroke-width=\"2\"\r\n              >\r\n                <path\r\n                  stroke-linecap=\"round\"\r\n                  stroke-linejoin=\"round\"\r\n                  d=\"M4 6h16M4 12h16M4 18h16\"\r\n                />\r\n              </svg>\r\n            </div>\r\n          </nav>\r\n        </header>\r\n        <main>\r\n          <div\r\n            class=\"slider h-[530px] bg-[url('./img/slider-bg.jpg')] bg-cover bg-no-repeat bg-bottom\"\r\n          >\r\n            <div\r\n              class=\"flex items-center justify-center w-full h-full bg-gray-900 bg-opacity-50\"\r\n            >\r\n              <div class=\"mx-16 text-center text-white\">\r\n  